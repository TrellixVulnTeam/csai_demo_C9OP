<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WindowPDF" WindowState="Maximized" Background="WhiteSmoke" Closed="windowClosed">
    <Grid x:Name="MainGrid">
        <UserControl>
            <ScrollViewer PanningMode="Both"
                Background="WhiteSmoke">
                <ItemsControl x:Name="PagesContainer"/>
            </ScrollViewer>
        </UserControl>
        <Button Content="Print" Click="printBtn_Click"  HorizontalAlignment="Left" VerticalAlignment="Top" Width="70" Margin="23,24,0,0" Height="39" Background="#FFE65100" BorderBrush="WhiteSmoke" Foreground="WhiteSmoke" FontFamily="Roboto Medium" FontSize="18" Panel.ZIndex="5"/>
    </Grid>
</Window>
